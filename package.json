{
  "name": "meteor-boilerplate",
  "private": true,
  "version": "0.1.5",
  "scripts": {
    "start": ".\\.scripts\\startMeteor.cmd local",
    "eslint-test": "eslint . --ignore-path .gitignore",
    "eslint-fix": "eslint . --ignore-path .gitignore --fix",
    "test": "meteor test --driver-package practicalmeteor:mocha --port 3050",
    "setup": ".\\.scripts\\setupMeteor.cmd",
    "startRemote": ".\\.scripts\\startMeteor.cmd local",
    "startLocal": ".\\.scripts\\startMeteor.cmd local",
    "push-blue": ".\\.scripts\\cfBlue.cmd",
    "push-green": ".\\.scripts\\cfGreen.cmd",
    "push-all": ".\\.scripts\\cfBlue.cmd && .\\.scripts\\cfGreen.cmd",
    "build": "meteor npm update && node .\\.scripts\\buildMeteor.js",
    "deploy-blue": "meteor npm update && node .\\.scripts\\buildMeteor.js && .\\.scripts\\cfBlue.cmd",
    "deploy-green": "meteor npm update && node .\\.scripts\\buildMeteor.js && .\\.scripts\\cfGreen.cmd",
    "map-blue": ".\\.scripts\\cfMapBlue.cmd",
    "map-green": ".\\.scripts\\cfMapGreen.cmd"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.51",
    "babel-runtime": "6.18.0",
    "bcrypt": "^5.0.0",
    "core-js": "^2.5.7",
    "g": "^2.0.1",
    "json-bigint": "^0.3.0",
    "lodash": "^4.17.13",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.22.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-bootstrap": "^0.31.2",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^15.5.4",
    "react-flip-move": "^2.10.2",
    "react-fontawesome": "^1.6.1",
    "react-markdown": "^3.3.4",
    "react-modal": "^2.2.1",
    "react-router": "^3.2.1",
    "react-select": "^1.2.1",
    "react-spinkit": "^3.0.0",
    "riek": "^1.1.0",
    "simpl-schema": "^1.5.0",
    "underscore": "^1.9.1"
  },
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 5.10.0"
  },
  "devDependencies": {
    "classnames": "^2.2.6",
    "enzyme": "^2.7.1",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.10.0",
    "expect": "^1.20.2",
    "react": "^15.6.2",
    "react-ace": "^6.1.3",
    "react-addons-test-utils": "^15.4.1"
  }
}
